; execute this like
; [assault1,helo] exec "eject.sqs";

_team = _this select 0;
_helo = _this select 1;

_aunits = units _team
_i = 0
_j = count _aunits

#Here
(_aunits select _i) action ["EJECT",_helo]
unassignvehicle (_aunits select _i)
_i=_i+1
~1
?_j>_i:goto "Here"

_helo flyinheight 50;

exit
