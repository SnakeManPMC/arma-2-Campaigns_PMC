;
; eject the russian squads out if hind
;

{unassignvehicle _x} foreach units rusinf8;

;
; eject squad rusinf8
;
_A = 0;
#WAITING
_listunits = units rusinf8;
_B = count _listunits
~.3
_listunits select _A action ["eject",helo2];
_A=_A+1;
?(_B > _A): goto "WAITING"

helo2 flyinheight 25;

Paratroops_Out=true;

exit
